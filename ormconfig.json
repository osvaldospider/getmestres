

{
    "type": "postgres",
    "host": "localhost",
    "port": 5432,
    "username": "postgres",
    "password": "admin",
    "database": "getmestres",
    "synchronize": true,
    "logging": false,
    "entities": [
      "src/entity/**/*.ts"
